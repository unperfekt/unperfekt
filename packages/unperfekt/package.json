{
  "name": "unperfekt",
  "version": "1.0.0",
  "description": "Unperfekt Design System Components",
  "homepage": "https://github.com/unperfekt/unperfekt#readme",
  "bugs": {
    "url": "https://github.com/unperfekt/unperfekt/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unperfekt/unperfekt.git",
    "directory": "packages/unperfekt"
  },
  "license": "MIT",
  "author": "Martin Broder <hello@martinbroder.com> (https://martinbroder.com)",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./css/*": "./css/*"
  },
  "main": "./dist/cjs/index.js",
  "svelte": "./src/index.ts",
  "unpkg": "./dist/custom-elements.js",
  "module": "./dist/esm/index.mjs",
  "types": "./types/index.d.ts",
  "files": [
    "dist/esm",
    "dist/cjs",
    "src",
    "types",
    "css",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:types": "node scripts/types.cjs",
    "build:watch": "rollup -c -w",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "rollup -c -w",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "dependencies": {
    "@floating-ui/dom": "^0.4.5",
    "@sveltejs/adapter-auto": "^1.0.0-next.40",
    "@sveltejs/kit": "^1.0.0-next.324",
    "classnames": "^2.3.1",
    "svelte": "^3.48.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "eslint-config-unperfekt": "workspace:*",
    "rollup": "^2.71.1",
    "rollup-plugin-svelte": "^7.1.0",
    "sveld": "^0.15.1",
    "svelte-check": "^2.7.0",
    "svelte-preprocess": "^4.10.6",
    "svelte2tsx": "^0.5.9",
    "tsconfig-unperfekt": "workspace:*",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "packageManager": "pnpm@7.0.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
