{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "lib/**", ".next/**"]
    },
    "deploy": {
      "dependsOn": ["build", "test"]
    },
    "test": {
      "dependsOn": ["lint", "build"]
    },
    "lint": {},
    "dev": {
      "cache": false
    }
  }
}
